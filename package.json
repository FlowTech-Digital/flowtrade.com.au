  "devDependencies": {
    "@opennextjs/cloudflare": "^1.0.0",
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
