{
  "name": "flowtrade",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:cf": "npx @opennextjs/cloudflare build",
    "build:next": "next build",
    "cf:build": "npx @opennextjs/cloudflare build",
    "cf:preview": "npx wrangler pages dev",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:generate": "supabase gen types typescript --local > src/lib/database.types.ts",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:api": "vitest run tests/api",
    "test:all": "npm run test:api && npm run test:e2e",
    "test:ci": "npm run test:api && playwright test --reporter=github"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-alert-dialog": "^1.1.2",
